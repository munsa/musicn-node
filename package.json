{
  "name": "owltown",
  "version": "1.0.0",
  "description": "Acoustic fingerprint social network",
  "main": "server.js",
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/react": "^16.8.23",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "form-data": "^2.5.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.6",
    "multer": "^1.4.2",
    "request": "^2.88.0",
    "server": "^1.0.19",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5",
    "wav-encoder": "^1.3.0"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "start": "node server",
    "server": "nodemon ./app/app.ts",
    "tsc": "tsc",
    "dev-server": "ts-node-dev --respawn --transpileOnly ./app/app.ts",
    "prod": "tsc && node ./build/app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
